<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextureView
        android:id="@+id/textureView2"
        android:layout_width="429dp"
        android:layout_height="96dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.333"
        app:layout_constraintStart_toStartOf="parent" />

    <TextureView
        android:id="@+id/textureView"
        android:layout_width="424dp"
        android:layout_height="73dp"
        app:layout_constraintEnd_toEndOf="@+id/Basket_button"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextureView
        android:id="@+id/textureView3"
        android:layout_width="428dp"
        android:layout_height="43dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textureView" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="240dp"
        android:layout_height="51dp"
        android:layout_marginEnd="171dp"
        android:layout_marginRight="171dp"
        android:text="Faire-Part Fait"
        android:textColor="#FFA407"
        android:textSize="36sp"
        app:layout_constraintBottom_toBottomOf="@+id/textureView"
        app:layout_constraintEnd_toEndOf="@+id/textureView"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/textureView" />

    <Button
        android:id="@+id/Connexion_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="24dp"
        android:text="Connexion"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/info_button"
        app:layout_constraintHorizontal_bias="0.219"
        app:layout_constraintStart_toStartOf="@+id/textureView2"
        app:layout_constraintTop_toTopOf="@+id/textureView2"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/info_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="36dp"
        android:layout_marginRight="36dp"
        android:text="info"
        app:layout_constraintBottom_toBottomOf="@+id/Connexion_button"
        app:layout_constraintEnd_toStartOf="@+id/Contact_button"
        app:layout_constraintTop_toTopOf="@+id/Connexion_button"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/Contact_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="24dp"
        android:layout_marginRight="24dp"
        android:layout_marginBottom="25dp"
        android:text="Contact"
        app:layout_constraintBottom_toBottomOf="@+id/textureView2"
        app:layout_constraintEnd_toEndOf="@+id/textureView2"
        app:layout_constraintTop_toTopOf="@+id/textureView2"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/Basket_button"
        android:layout_width="76dp"
        android:layout_height="67dp"
        android:background="@drawable/kisspng_computer_icons_logo_basket_symbol_5b37b29fb281c0_8388649915303768637312"
        app:layout_constraintBottom_toTopOf="@+id/textureView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

</androidx.constraintlayout.widget.ConstraintLayout>